{
  "name": "buildahcker-alpine-sample",
  "type": "module",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.2",
    "buildahcker": "0.6.2",
    "tsx": "^4.7.1"
  },
  "scripts": {
    "build": "buildah unshare tsx build.ts",
    "start": "qemu-system-x86_64 -m 1024M -enable-kvm -machine q35 -device intel-iommu -device virtio-scsi-pci,id=scsi0,num_queues=4 -device scsi-hd,drive=drive0,bus=scsi0.0,channel=0,scsi-id=0,lun=0 -drive file=output/qemu/disk.img,format=raw,if=none,id=drive0,readonly=on -nic user,model=virtio-net-pci,hostfwd=tcp::55022-:22"
  }
}
